{
  "name": "@soapbubble/core",
  "version": "1.1.3",
  "description": "The root soapbubble.online site",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CaptEmulation/soapbubble.online.git"
  },
  "main": "lib/server.js",
  "author": "Halt Hammerzeit",
  "license": "MIT",
  "scripts": {
    "start": "npm run production",
    "start:dev": "npm-run-all development:client:rendering-service:build:prepare development",
    "development": "npm-run-all --parallel development:client:build development:client:rendering-service:build development:services",
    "development:client:rendering-service:build:prepare": "universal-webpack --settings ./client/webpack/universal-webpack-settings.js --config ./client/webpack/webpack.config.js prepare",
    "development:client:build": "babel-node ./client/webpack/webpack-dev-server.js",
    "development:client:rendering-service:build": "webpack --config ./client/webpack/webpack.config.server.development.babel.js --watch --colors --display-error-details",
    "development:api": "better-npm-run development:api",
    "development:client:rendering-service": "better-npm-run development:client:rendering-service",
    "development:client:rendering-service:delayed": "npm-run-all delay development:client:rendering-service",
    "development:webserver": "better-npm-run development:webserver",
    "development:webserver:delayed": "npm-run-all delay delay development:webserver",
    "development:services": "npm-run-all --parallel development:client:rendering-service:delayed development:webserver:delayed",
    "delay": "node ./sleep 500",
    "production": "npm-run-all production:client:build production:client:rendering-service:build production:services",
    "production:client:build": "webpack --colors --display-error-details --config ./client/webpack/webpack.config.client.production.babel.js",
    "production:client:rendering-service:build": "webpack --colors --display-error-details --config ./client/webpack/webpack.config.server.production.babel.js",
    "production:api": "better-npm-run production:api",
    "production:client:rendering-service": "better-npm-run production:client:rendering-service",
    "production:webserver": "better-npm-run production:webserver",
    "production:services": "npm-run-all --parallel production:client:rendering-service production:webserver",
    "build:production": "npm run production:client:build && npm run production:client:rendering-service:build"
  },
  "betterScripts": {
    "production:client:rendering-service": {
      "command": "node ./client/rendering-service/entry.js"
    },
    "production:api": {
      "command": "node ./api/entry.js"
    },
    "production:webserver": {
      "command": "node ./webserver/entry.js"
    },
    "development:client:rendering-service": {
      "command": "nodemon ./client/rendering-service/entry.js --watch ./client/rendering-service --watch ./client/build/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "development:api": {
      "command": "nodemon ./api/entry.js --watch ./api",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "development:webserver": {
      "command": "nodemon ./webserver/entry.js --watch ./webserver",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.6.1",
    "axios": "^0.16.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "detect-node": "^2.0.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "font-awesome": "https://registry.npmjs.org/font-awesome/-/font-awesome-4.7.0.tgz",
    "fs-extra": "^2.0.0",
    "google-auth-library": "^0.11.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.1.1",
    "npm-run-all": "^4.0.0",
    "postcss-calc": "^5.3.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-loader": "^1.2.1",
    "prop-types": "https://registry.npmjs.org/prop-types/-/prop-types-15.5.10.tgz",
    "react": "^15.5.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.5.0",
    "react-google-login": "^2.9.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-isomorphic-render": "^9.1.0",
    "react-redux": "^5.0.1",
    "react-responsive-ui": "^0.1.44",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^4.1.1",
    "simpler-redux-form": "^0.3.1",
    "style-loader": "^0.13.1",
    "universal-webpack": "^0.2.2",
    "url-loader": "^0.5.7",
    "web-service": "^0.2.0",
    "webpack": "^2.2.0-rc.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "https://registry.npmjs.org/babel-plugin-transform-object-rest-spread/-/babel-plugin-transform-object-rest-spread-6.22.0.tgz",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "less": "^2.3.1",
    "less-loader": "^4.0.5",
    "nodemon": "^1.11.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "svg-react-loader": "^0.3.7",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
